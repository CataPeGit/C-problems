#include <iostream>
#include <cmath>
#include <fstream>
using namespace std;



int main()
{
    int i , j , n;
    cin >> n;
    int a[101][101] = {0};
    int k = 1;
    for(i = 1; i <= n; i++)
    {
        for(j = 1; j <= n; j++)
        {
            a[i][j] = k;
            if(k == 9)
                k = 1;
            else
                k++;
        }
    }


    for(i = 1; i <= n; i++)
    {
        for(j = 1; j <= n; j++)
            cout << a[i][j] << " ";
        cout << endl;
    }
    return 0;
}


