sub 2 ex 3:
#include <iostream>
#include <cstring>
using namespace std;

int main()
{
    char s[20];
    int p;
    cin.get(s,20);
    for(p = 0; s[p] != '\0'; p++)
        if(s[p] == 'A' && s[p-1] == 'I');
        else if(s[p] == 'A' && s[p+1] == 'I');
        else
            cout << s[p];
    return 0;
}

sub 3 ex 2:
#include <iostream>
#include <cstring>
#include <fstream>
using namespace std;


int main()
{
    unsigned int n, i, j, a[25][25];
    cin >> n;
    for(i = 1; i <= n; i++)
        for( j = 1; j <= n; j++)
        {
            if(i + j == n + 1)
                a[i][j] = 0;
            else if(i + j < n + 1)
                a[i][j] = n - i - j + 1;
            else if(i + j > n + 1)
                a[i][j] = (n - i - j + 1) * (-1);
        }
    for(i = 1; i <= n; i++)
    {
        for( j = 1; j <= n; j++)
            cout << a[i][j] << " ";
        cout << endl;
    }
    return 0;
}

sub 3 ex 3:
#include <iostream>
#include <cstring>
#include <fstream>
using namespace std;

int main()
{
    ifstream f("bac.in");
    int n, ok, poz1, poz2, totalNr = 0;
    ok = 0;
    while(f >> n)
    {
        totalNr++;
        if(n < 0 && ok == 0)
        {
            poz1 = totalNr;
            ok = 1;
        }
        if(n < 0 && ok == 1)
            poz2 = totalNr;
    }
    if(totalNr - poz1 > totalNr - poz2)
        cout << totalNr - poz1 + 1;
    else
        cout << totalNr - poz2 + 1;
    return 0;
}


