ex 3:
#include <iostream>
#include <fstream>
using namespace std;

void tablou(unsigned int n, unsigned int k, unsigned int a[200])
    {
        int aux = 0;
        for(int i = 1; i <= n; i++)
            for(int j = 1; j <= n; j++)
            {
                a[aux] = i;
                aux++;
            }
    }

int main()
{
    unsigned int n, k , a[200];
    cin >> n >> k;
    tablou(n,k,a);
    cout <<endl;
    for(int i = 1; i <= n * k; i++)
        cout << a[i] << " ";
    return 0;
}
