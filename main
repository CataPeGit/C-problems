//var 12
int pDoi(int n)
{
    int e = 0;
    int d = 2;
    while(n != 1)
    {
        while(n % d == 0)
        {
            e++;
            n = n / d;
        }

    }
}

bool ePatrat(int numar)
{
    if((int)sqrt(numar) == sqrt(numar))
        return true;
    return false;
}

void patrate(int x, int y)
{
    int s = 0;
    int k = 0;
    int ok = 0;
    for(int i = x ; i <= y; i++)
    {
        if(ePatrat(i))
            k++;
    }
    for(int i = x ; i <= y; i++)
    {
        if(ok == k)
            if(ePatrat(i))
                {
                    s = s +i;
                    cout << i;
                }
        else
        {
            if(ePatrat(i))
                {
                    s = s +i;
                    cout << i <<"+";
                }
        }
    }
    cout << "=" << s;
    cout << k;
}

void suma(int n , int &s)
{
    s = 0;
    int v[10] = {0};
    while(n > 0)
    {
        int uc = n % 10;
        for(int i = 1; i < 10 ; i= i + 2)
        {
            if(i == uc)
                v[i]++;
        }
        n = n / 10;
    }
    for(int i = 1; i < 10 ; i = i + 2)
        if(v[i] > 0)
            s = s + i;
}

/* ceva de la un test
int putere(int n, int p)
{
    int div = 2; //luam o variabila divizor
    int exp = 0; //luam o variabila exponent
    while(n != 1) // incepem descompunerea in factori primi pana ajung la 1
    {
        while(n % div == 0) // descompunem in factori primi
        {
            exp++; //marim exponentul cu 1
            n = n / div; //impartim cu divizorul
        }
        if(p == div) // in cazul in care parametrul p este egal cu divizorul cautat returnam exponentul
            {
                return exp;
            }
        div++; //crestem divizorul
        exp=0; // resetam exponentul
    }
    return -1; // in cazul contrar returnam -1
}
