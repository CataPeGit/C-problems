sub 3 ex 1 :
void generatoare(unsigned int n )
{
    int ok = 0;
    for(int a = 2; a <= n / 2; a = a + 2)
        for(int b = 1; b <= n / 2; b++)
            if( a * b + a / b == n)
            {
                ok = 1;
                cout << a << "-" << b << " ";
            }
    if(ok == 0)
        cout << "nu exista" ;
}
int main()
{
    unsigned int n ;
    cin >> n;
    generatoare(n);
    return 0;
}

