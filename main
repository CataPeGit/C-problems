#include <iostream>
#include <fstream>
using namespace std;
int main()
{
    int n , aux = -1;
    ifstream fin("bac.txt");
    while(fin >> n)
    {
        if(n % 2 == 0 && n != aux)
        {
            aux = n;
            cout << n <<" ";
        }
    }
    fin.close();
    return 0;
}


