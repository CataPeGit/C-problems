//#include <bits/stdc++.h>
#include <iostream>
#include <cmath>
#include <fstream>
using namespace std;


int main()
{
    int i , j , n;
    cin >> n;
    int a[101][101] = {0};
    for(i = 1; i <= n; i++)
        for(j = 1; j <= n; j++)
            if(a[i][j] == 0)
            {
                a[i][j] = i;
                a[j][i] = i;
            }
    for(i = 1; i <= n; i++)
    {
        for(j = 1; j <= n; j++)
            cout << a[i][j];
        cout << endl;
    }


    return 0;
}









